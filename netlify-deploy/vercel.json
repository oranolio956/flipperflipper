{
  "name": "proxy-assessment-tool",
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "BACKEND_URL": "@backend_url"
  },
  "functions": {
    "api/config.js": {
      "maxDuration": 10
    }
  }
}